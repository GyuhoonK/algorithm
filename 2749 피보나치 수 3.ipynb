    "#피사노 주기#\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "sys.setrecursionlimit(10**6)\n",
    "\n",
    "NUM = 10 ** 6\n",
    "CYCLE = 15 * (10 ** (6-1))\n",
    "\n",
    "n = int(input())\n",
    "n %= CYCLE\n",
    "\n",
    "f = [0,1]\n",
    "for i in range(2,1+n):\n",
    "    f.append(((f[i-1] % NUM) + (f[i-2] % NUM)) % NUM)\n",
    "print(f[n])"
   ]
  }
